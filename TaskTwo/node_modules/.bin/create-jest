#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/henrique/Desktop/HNG_Backend/HNG_Backend/TaskTwo/node_modules/create-jest/bin/node_modules:/Users/henrique/Desktop/HNG_Backend/HNG_Backend/TaskTwo/node_modules/create-jest/node_modules:/Users/henrique/Desktop/HNG_Backend/HNG_Backend/TaskTwo/node_modules:/Users/henrique/Desktop/HNG_Backend/HNG_Backend/node_modules:/Users/henrique/Desktop/HNG_Backend/node_modules:/Users/henrique/Desktop/node_modules:/Users/henrique/node_modules:/Users/node_modules:/node_modules:/Users/henrique/Desktop/HNG_Backend/HNG_Backend/TaskTwo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/henrique/Desktop/HNG_Backend/HNG_Backend/TaskTwo/node_modules/create-jest/bin/node_modules:/Users/henrique/Desktop/HNG_Backend/HNG_Backend/TaskTwo/node_modules/create-jest/node_modules:/Users/henrique/Desktop/HNG_Backend/HNG_Backend/TaskTwo/node_modules:/Users/henrique/Desktop/HNG_Backend/HNG_Backend/node_modules:/Users/henrique/Desktop/HNG_Backend/node_modules:/Users/henrique/Desktop/node_modules:/Users/henrique/node_modules:/Users/node_modules:/node_modules:/Users/henrique/Desktop/HNG_Backend/HNG_Backend/TaskTwo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../create-jest/bin/create-jest.js" "$@"
else
  exec node  "$basedir/../create-jest/bin/create-jest.js" "$@"
fi
