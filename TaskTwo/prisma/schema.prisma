model User {
  userId       String        @id @default(uuid())
  firstName    String
  lastName     String
  email        String        @unique
  password     String
  phone        String?
  organisations Organisation[] @relation("UserToOrganisation") @map("organizations")
}

model Organisation {
  orgId       String        @id @default(uuid())
  name        String
  description String?
  users       User[]        @relation("UserToOrganisation") @map("users")
}
